<h1>Edit Band</h1>

<form action="<%=band_url(@band)%> "method="POST">
  <input type="hidden" name="_method" value="PATCH">

  <%= auth_token %>

  <label for="name">Band Name</label>
  <input type="text" id="name" name=band[name] value="<%=@band.name%>">
  <br>

  <input type="submit" value="Update Band">

</form>